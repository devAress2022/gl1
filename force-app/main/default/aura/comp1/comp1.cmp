<aura:component controller="comp1" implements='flexipage:availableForAllPageTypes' access="global" >
      
    <aura:attribute type="Customer1__c[]" name="acctList"/>
    <aura:attribute name="mycolumns" type="List"/>
      
    <aura:handler name="init" value="{!this}" action="{!c.fetchAcc}"/>
      
    <lightning:datatable data="{! v.acctList }"
                         columns="{! v.mycolumns }"
                         keyField="id"
                         CheckboxColumn="True"
                         />
      
</aura:component>